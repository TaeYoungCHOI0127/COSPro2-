#include <stdio.h>

int main(void) 
{                                             안녕하세여
    int num1 = 12;
    int num2 = 12;
                                        
    printf("num1 : %d \n", num1); // num1의 현재 값 출력
    printf("num1++ : %d \n", num1++); // 후위 증가 연산자를 사용하여 num1을 출력 후 1 증가
    printf("num1 : %d \n", num1); // 후위 증가 연산자를 사용했으므로 이전에 출력된 값에서 1이 증가된 값 출력
    printf("num1++ : %d \n", ++num1); // 전위 증가 연산자를 사용하여 num1을 1 증가시킨 후 출력

    /*
    int num1 =  3, num2 = 4;
    int result1 = num1 + num2;
    int result2 = num1 - num2;
    int result3 = num1 * num2;
    int result4 = num1 / num2;
    */

    // num1 = num2 = 0;

    /*
    printf("num1 : %d, num2 : %d \n", num1, num2);
    printf("%d + %d = %d \n", num1, num2, result1);
    printf("%d - %d = %d \n", num1, num2, result2);
    printf("%d * %d = %d \n", num1, num2, result3);
    printf("%d / %d = %d \n", num1, num2, result4);
    */

    /*
    int num;
    num = 20;
    printf("%d \n", num);
    */
    
    return 0; // 프로그램 종료
}
